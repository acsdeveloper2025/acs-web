# Production Environment Variables for ACS Web Frontend

# API Configuration
VITE_API_URL=https://api.yourdomain.com
VITE_WS_URL=wss://api.yourdomain.com

# Application Configuration
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=ACS Web Frontend
VITE_APP_DESCRIPTION=Advanced Case Management System - Web Frontend

# Production Settings
NODE_ENV=production

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_HOT_RELOAD=false

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false

# Development Tools (Disabled in Production)
VITE_ENABLE_REDUX_DEVTOOLS=false
VITE_ENABLE_REACT_QUERY_DEVTOOLS=false

# Authentication (Production)
VITE_AUTH_DOMAIN=yourdomain.com
VITE_AUTH_CLIENT_ID=prod-client-id

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,doc,docx

# Maps and Location
VITE_GOOGLE_MAPS_API_KEY=your-prod-google-maps-api-key
VITE_DEFAULT_LATITUDE=40.7128
VITE_DEFAULT_LONGITUDE=-74.0060

# Notifications
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_NOTIFICATION_TIMEOUT=3000

# Performance
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_BUNDLE_ANALYZER=false

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_ENABLE_ANALYTICS=true

# CDN
VITE_CDN_URL=https://cdn.yourdomain.com
VITE_STATIC_ASSETS_URL=https://static.yourdomain.com
