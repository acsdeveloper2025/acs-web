# Development Environment Variables for ACS Web Frontend

# API Configuration
VITE_API_BASE_URL=http://localhost:3000/api
VITE_WS_URL=ws://localhost:3000

# Application Configuration
VITE_APP_VERSION=dev
VITE_APP_NAME=ACS Web Frontend
VITE_APP_DESCRIPTION=Advanced Case Management System - Web Frontend

# Development Settings
NODE_ENV=development
VITE_DEV_SERVER_HOST=0.0.0.0
VITE_DEV_SERVER_PORT=5173

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_HOT_RELOAD=true

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true

# Development Tools
VITE_ENABLE_REDUX_DEVTOOLS=true
VITE_ENABLE_REACT_QUERY_DEVTOOLS=true

# Authentication (Development)
VITE_AUTH_DOMAIN=dev.yourdomain.com
VITE_AUTH_CLIENT_ID=dev-client-id

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,doc,docx

# Maps and Location
VITE_GOOGLE_MAPS_API_KEY=your-dev-google-maps-api-key
VITE_DEFAULT_LATITUDE=40.7128
VITE_DEFAULT_LONGITUDE=-74.0060

# Notifications
VITE_ENABLE_PUSH_NOTIFICATIONS=false
VITE_NOTIFICATION_TIMEOUT=5000

# Performance
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_BUNDLE_ANALYZER=false
